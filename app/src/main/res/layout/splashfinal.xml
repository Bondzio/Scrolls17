<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:background="#FFFFFF"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline44"
        app:layout_constraintGuide_percent="0"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline45"
        app:layout_constraintGuide_percent="1.0"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="360dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline46"
        app:layout_constraintGuide_percent="0.0"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />
    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline49"
        app:layout_constraintGuide_percent="0.0"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline47"
        app:layout_constraintGuide_percent="0.3"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="161dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline48"
        app:layout_constraintGuide_percent="0.85"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="429dp"
        tools:layout_editor_absoluteX="0dp" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline50"
        app:layout_constraintGuide_percent="1.0"
        android:orientation="horizontal"
        tools:layout_editor_absoluteY="536dp"
        tools:layout_editor_absoluteX="0dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        app:srcCompat="@drawable/landingpagescrolls"
        app:layout_constraintRight_toLeftOf="@+id/guideline45"
        app:layout_constraintLeft_toLeftOf="@+id/guideline44"
        app:layout_constraintBottom_toTopOf="@+id/guideline47"
        app:layout_constraintTop_toTopOf="@+id/guideline49" />


    <android.support.constraint.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="0dp"

        app:layout_constraintRight_toLeftOf="@+id/guideline45"
        app:layout_constraintLeft_toLeftOf="@+id/guideline44"
        app:layout_constraintBottom_toTopOf="@+id/guideline48"
        app:layout_constraintTop_toTopOf="@+id/guideline47"
        >
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline441"
            app:layout_constraintGuide_percent="0.0"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="56dp"
            tools:layout_editor_absoluteX="0dp" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline442"
            app:layout_constraintGuide_percent="0.2"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="56dp"
            tools:layout_editor_absoluteX="144dp" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline443"
            app:layout_constraintGuide_percent="0.85"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="56dp"
            tools:layout_editor_absoluteX="288dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline451"
            app:layout_constraintGuide_percent="1.0"
            android:orientation="vertical"
            tools:layout_editor_absoluteY="56dp"
            tools:layout_editor_absoluteX="360dp" />

        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline461"
            app:layout_constraintGuide_percent="0.0"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="56dp"
            tools:layout_editor_absoluteX="0dp" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline491"
            app:layout_constraintGuide_percent="1.0"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="324dp"
            tools:layout_editor_absoluteX="0dp" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline4611"
            app:layout_constraintGuide_percent="0.3"
            android:orientation="horizontal"
            tools:layout_editor_absoluteY="136dp"
            tools:layout_editor_absoluteX="0dp" />
        <android.support.constraint.Guideline
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/guideline4911"
            app:layout_constraintGuide_percent="0.8"
            android:orientation="horizontal"
          />



        <WebView
            android:id="@+id/webView"
            android:layout_width="0dp"
            android:layout_height="0dp"

            android:layout_marginRight="0dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="0dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="0dp" />
        <WebView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintRight_toLeftOf="@+id/guideline443"
            android:layout_marginRight="0dp"
            android:id="@+id/web_view"
            android:layout_marginLeft="0dp"
            app:layout_constraintLeft_toLeftOf="@+id/guideline442"
            android:layout_marginBottom="0dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline4911"
            app:layout_constraintTop_toTopOf="@+id/guideline4611"
            android:layout_marginTop="0dp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="1.0" />

    </android.support.constraint.ConstraintLayout>
    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:src="@drawable/poweredby"
        app:layout_constraintRight_toLeftOf="@+id/guideline45"
        app:layout_constraintLeft_toLeftOf="@+id/guideline44"
        app:layout_constraintBottom_toTopOf="@+id/guideline50"
        app:layout_constraintTop_toTopOf="@+id/guideline48"/>


</android.support.constraint.ConstraintLayout>